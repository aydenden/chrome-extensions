{
  "manifest_version": 3,
  "name": "My Wanted Helper",
  "version": "1.0.0",
  "description": "원티드 페이지를 보조하는 확장 프로그램",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.wanted.co.kr/*"],
      "js": ["dist/index.js"]
    }
  ]
}